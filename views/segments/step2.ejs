      <!-- For popup design-->

    <div class="modal-bg" id="popupFamilyHistory" style="display: none" >
      <div class="modal-container" > 
        <div class="card">
          <p id='calledFrom' style="display: none" ></p>
          <label class="fieldlabels" for="famimilyMemberName">Name:</label>
          <input type="text" name="name" id='famimilyMemberName' placeholder="" cols="5"/>     
          <div class="conditional">
            <label class="fieldlabels" for="famimilyMemberGender">Gender:</label>
            <input type="text" name="gender" id='famimilyMemberGender' placeholder="" cols="5"/>
          </div>   
          <label class="fieldlabels" for="famimilyMemberBirthDate">Birth date:</label>
          <input type="text" name="birthDate" id='famimilyMemberBirthDate' placeholder="" cols="5"/>
