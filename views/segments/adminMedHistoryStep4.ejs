    <script>
      $.get("/admin/profile/getSectionData/Education and Occupation Details", (data) => {
      let elmStr = "";
      for (let i = 0; i < data.subSections.length; i++) {
        elmStr += getSubSectionStr(data.subSections[i]);
      }
      $("#addPlace4").replaceWith(elmStr);
    });
    </script>
